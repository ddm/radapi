    {
        "id": "29af73e9.d973fc",
        "type": "function",
        "z": "127bc0ee.6df25f",
        "name": "draw rectangle on OLED",
        "func": "var i2c = global.get(\"i2c\");\nvar i2cBus = i2c.openSync(1);\n\nconst SIZE_X=128;\nconst SIZE_Y=64;\n\nvar opts = {\n  width: SIZE_X,\n  height: SIZE_Y,\n  address: 0x3C\n};\n\nvar Oled = global.get(\"oled\");\nvar oled = new Oled(i2cBus, opts);\n\noled.clearDisplay();\noled.turnOnDisplay();\n\noled.drawPixel([\n    [SIZE_X-1, 0, 1],\n    [SIZE_X-1, SIZE_Y-1, 1],\n    [0, SIZE_Y-1, 1],\n    [0, 0, 1]\n]);\n\noled.drawLine(1, 1, SIZE_X-2, 1, 1);\noled.drawLine(SIZE_X-2, 1, SIZE_X-2, SIZE_Y-2, 1);\noled.drawLine(SIZE_X-2, SIZE_Y-2, 1, SIZE_Y-2, 1);\noled.drawLine(1, SIZE_Y-2, 1, 1, 1);\n\nmsg.payload = \"You should see a rectangle on the screen üëç\";\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "x": 370,
        "y": 120,
        "wires": [
            []
        ]
    },
    {
        "id": "657b23f0.ccdf6c",
        "type": "function",
        "z": "127bc0ee.6df25f",
        "name": "shutdown OLED",
        "func": "var I2C = global.get(\"i2c\");\nvar Oled = global.get(\"oled\");\n\nvar oled = new Oled(I2C.openSync(1), {\n  width: 128,\n  height: 64,\n  address: 0x3C\n});\n\noled.clearDisplay();\noled.turnOffDisplay();\n\nmsg.payload = \"Screen should be off üëç\"\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "x": 360,
        "y": 20,
        "wires": [
            []
        ]
    },
