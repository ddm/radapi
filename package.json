{
  "name": "radapi",
  "version": "0.0.0",
  "description": "Rapid API development with Node-RED",
  "author": "Dimitri del Marmol",
  "license": "MIT",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/ddm/radapi.git"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=6"
  },
  "main": "index.js",
  "bin": {
    "radapi": "index.js"
  },
  "scripts": {
    "preinstall": "ncu -ua",
    "postinstall": "bower install && nsp check --warn-only --output summary",
    "start": "node index"
  },
  "dependencies": {
    "async": "2.1.4",
    "bower": "1.8.0",
    "express": "4.14.1",
    "node-red": "0.16.2",
    "node-red-node-swagger": "0.1.8",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "npm-check-updates": "2.10.2",
    "nsp": "2.6.2",
    "nsp-formatter-checkstyle": "1.0.1"
  }
}
